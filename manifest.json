{
  "manifest_version": 2,
  "name": "2dbc",
  "short_name": "2dbc",
  "version": "0.1",
  "homepage_url": "http://littleplasticsquares.com/2dbc",
  "description": "Shows a 2D barcode for the URL of the current tab.",
  "author": "Sam Moorhouse",
  "icons": { 
    "32": "2dbc_32.png",
    "48": "2dbc_48.png",
    "128": "2dbc_128.png"
  },
  "offline_enabled": false,
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": {
      "128": "2dbc_128.png"
    },
    "default_title": "2DBC",
    "default_popup": "popup.html"
  }
}